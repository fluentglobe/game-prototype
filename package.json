{
  "name": "fluent-play",
  "version": "0.1.0",
  "description": "Fluent Play library",
  "repository": {
    "type": "git",
    "url": "git@github.com:fluentglobe/fluent-play.git"
  },
  "readme": "README.md",
  "license": "AGPL",
  "private": true,
  "scripts": {
    "lint": "jshint src/**/*.js",
    "start": "node _demo/server start",
    "stop": "node _demo/server stop",
    "build:html": "ncp src/index.html dist/index.html",
    "build:assets": "ncp assets dist/assets",
    "prebuild:js": "npm run lint",
    "build:js": "browserify src/main.js | uglifyjs > dist/main.min.js",
    "prebuild": "npm run clean",
    "build": "npm run build:html && npm run build:assets && npm run build:js",
    "test": "mocha common-game-control/**/*.spec.js connect-dots/**/*.spec.js dial-number/**/*.spec.js tell-time/**/*.spec.js",
    "test2": "./node_modules/.bin/gulp test"
  },
  "author": "Henrik Vendelbo",
  "devDependencies": {
    "chai": "^2.2.0",
    "chokidar": "^1.2.0",
    "conventions": "^0.1.0",
    "express": "^4.13.3",
    "gulp": "^3.8.11",
    "gulp-browserify": "^0.5.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "^0.2.0",
    "gulp-mocha": "^2.1.3",
    "gulp-remember": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.4",
    "jade": "^1.9.2",
    "js-yaml": "^3.2.7",
    "lunr": "^0.5.7",
    "mkdirp": "^0.5.0",
