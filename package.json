    "node-sass": "^3.3.3",
    "semver": "^4.3.3",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "socketstream": "0.5.x",
    "socketstream-cookie-session": "0.5.x",
    "sockjs": "~0.3.15",
    "sockjs-client": "~1.0.3",
    "winston": "^0.9.0",
    "zombie": "^3.1.1"
  },
  "engines": {
    "node": ">=0.10.0",
    "steroids": "3.1.9"
  }
